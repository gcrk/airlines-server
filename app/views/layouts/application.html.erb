<!DOCTYPE html>
<html>
  <head>
    <title>AirlinesServer</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h2><%= link_to 'Airplanes', airplanes_path %></h2>
    <h2><%= link_to 'Flights', flights_path %></h2>
    <h2><%= link_to 'Reservations', reservations_path %></h2>
    <h2><%= link_to 'Users', users_path %></h2>

    <%= yield %>
  </body>
</html>
